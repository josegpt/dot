alias ll ls -lh
alias la ls -lah
alias rb doas reboot
alias po doas poweroff
alias sd doas shutdown
alias eqi cat /var/lib/portage/world
alias er doas emerge --deselect $*
alias ero doas emerge --depclean $*
alias eiu doas emerge -NuDa @world
alias eis doas emerge --sync $*
alias ei doas emerge $*
alias es emerge --search $*