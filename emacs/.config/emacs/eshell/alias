alias ll ls -lh
alias la ls -lah
alias rb doas reboot
alias sd doas shutdown now
alias pa doas pkg_add $*
alias pu doas pkg_add -u
alias pr doas pkg_delete $*
alias pro doas pkg_delete -a
alias pq pkg_info -Q $*
alias pqi pkg_info -m
alias pqd pkg_info -mz | tee list
alias gpgr gpg-connect-agent reloadagent /bye
alias nf neofetch --ascii_distro openbsd_small
