alias ll ls -lh
alias la ls -lah
alias prb doas reboot
alias pha doas halt -p
alias pa doas pkg_add $*
alias pd doas pkg_delete $*
alias pc doas pkg_check $*
alias pi pkg_info $*
alias rc doas rcctl $*
alias syp openrsync -a /usr/ports/packages/amd64/signed/* $2:/var/www/pub/OpenBSD/$1/packages/amd64/
alias snp doas pkg_sign -s signify2 -s /etc/signify/canal-pkg.sec -o /usr/ports/packages/amd64/signed -S /usr/ports/packages/amd64/all